{
  "manifest_version": 2,
  "name": "Kera Editor",
  "description": "Edit Kera Walkthroughs",
  "version": "1.0.4",
  "page_action": {
    "default_icon": "deactive.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentscript.js"],
    "all_frames": false
  }],
  "content_security_policy": "script-src 'self' https://s3.amazonaws.com http://localhost; object-src 'self'"
}
